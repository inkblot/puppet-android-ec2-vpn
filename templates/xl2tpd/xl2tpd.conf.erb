[global]
access control = no
<%- if @debug -%>
debug avp = yes
debug network = yes
debug state = yes
debug tunnel = yes
<%- end -%>

[lns default]
ip range = 192.168.200.100-192.168.200.110
local ip = 192.168.200.10
require authentication = yes
require chap = yes
refuse pap = yes
length bit = yes
name = l2tp
pppoptfile = /etc/ppp/options.xl2tpd
<%- if @debug -%>
ppp debug = yes
<%- end -%>
